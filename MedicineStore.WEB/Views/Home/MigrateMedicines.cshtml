
@{
    ViewData["Title"] = "MigrateMedicines";
}

<h1>MigrateMedicines</h1>

<div class="col-md-12 order-md-1">
    <form method="post" enctype="multipart/form-data" asp-action="MigrateMedicines">
        <div class="form-group">
            <div class="col-md-10">
                <p>Upload file:</p>
                <input type="file" name="files" />
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-10">
                <input type="submit" value="Upload" />
            </div>
        </div>
    </form>
</div>